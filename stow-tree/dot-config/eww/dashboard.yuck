(defwidget dashboard []
  (box :class "transparent" :orientation "h"
    (box :orientation "v"
      (box :orientation "h" "1" "2" "3")
      (box :orientation "h" "4" "5" "6"))
    (box :orientation "v"
      (box :orientation "h" :class "transparent" "7" "8" "9")
      (box :orientation "h" "10" "11" "12"))))

(defwindow dashboard
  :namespace "dashboard"
  :monitor 0
  :stacking "fg"
  :focusable "none"
  :geometry (geometry :x "10%" :y "10%" :width "80%" :height "80%")
  (dashboard))
